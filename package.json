{
	"name": "mikeOS",
	"version": "8.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^3.1.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "github:11ty/eleventy-plugin-bundle",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"luxon": "^3.5.0",
		"markdown-it-anchor": "^8.6.7"
	},
	"dependencies": {
		"@11ty/eleventy-upgrade-help": "^3.0.1",
		"@photogabble/eleventy-plugin-interlinker": "^1.0.5",
		"eleventy-plugin-activity-pub": "^0.1.1",
		"eleventy-plugin-bookwyrm": "^1.4.0",
		"eleventy-plugin-embed-everything": "^1.19.0",
		"eleventy-plugin-embed-spotify": "^1.3.0",
		"eleventy-plugin-indieweb": "^0.0.1",
		"eleventy-plugin-inline-link-favicon": "^1.1.0",
		"markdown-it": "^14.1.0",
		"markdown-it-table-of-contents": "^0.6.0",
		"markdown-it-task-checkbox": "^1.0.6",
		"markdown-it-wikilinks": "^1.4.0"
	}
}
